    "url": "https://github.com/Nightroit/chainer/issues"
  },
  "homepage": "https://github.com/Nightroit/chainer#readme",
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "axios": "^0.24.0",
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "crypto": "^1.0.1",
    "dotenv": "^11.0.0",
    "express": "^4.17.2",
    "helmet": "^5.0.1",
    "sha256": "^0.2.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "concurrently": "^7.0.0",
    "eslint": "^8.6.0",
    "nodemon": "^2.0.15"
  }
}
